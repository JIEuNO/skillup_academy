put :'',\r\n                items : [] ,\r\n            }\r\n        },\r\n        //함수 선언 방법 \r\n        // this.isMenuShow = !this.isMenuShow\r\n        //로 사용가능 \r\n        methods :{\r\n            addTodo(){\r\n                this.items.push(this.addInput)\r\n                this.addInput=\"\"\r\n            },\r\n            deleteTodo(index){\r\n              this.items.splice(index,1)\r\n            }\r\n            }\r\n        \r\n    }\r\n   \r\n     \r\n</script>\r\n\r\n<!-- CSS -->\r\n<style lang = \"scss\">\r\n\r\n.add-button{\r\n    display:inline-block;\r\n}\r\n.item{\r\n    .item-label {\r\n        display : inline-block;\r\n    }\r\n    .item-button {\r\n        display : inline-block;\r\n    }\r\n}\r\n</style>\r\n"},"lineCount":null},"css":".add-button,.item .item-button,.item .item-label{display:inline-block}"},"hash":"f49a5529c19dc256852b410f57e4fd71","cacheData":{"env":{}}}